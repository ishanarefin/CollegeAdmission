<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.yousuf.collegeadmission"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="19"
        android:targetSdkVersion="23" />

    <uses-permission android:name="android.permission.SEND_SMS" />

    <application
        android:name="com.android.tools.fd.runtime.BootstrapApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.yousuf.collegeadmission.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.yousuf.collegeadmission.With_All_Input"
            android:label="Apply" >
        </activity>
        <activity
            android:name="com.example.yousuf.collegeadmission.Dhaka_Board"
            android:label="Dhaka Board" >
        </activity>
        <activity
            android:name="com.example.yousuf.collegeadmission.Chittagong_Board"
            android:label="Chittagong Board" >
        </activity>
    </application>

</manifest>